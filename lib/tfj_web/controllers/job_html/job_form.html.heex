<.form :let={job_info} action={~p"/jobs/find_job"} method="post">
  <div class="grid lg:grid-cols-2 sm:grid-cols-1 gap-4 mb-4">
    <.input field={job_info[:user_linkedin]} type="text" label="Linkedin Profile Slug" />
    <.input field={job_info[:job_annoucement_link]} type="text" label="Job Announcement Link" />
  </div>
  <footer>
    <TfjComponents.button variant="primary">Go!</TfjComponents.button>
  </footer>
</.form>